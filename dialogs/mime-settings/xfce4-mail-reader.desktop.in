[Desktop Entry]
Version=1.0
Type=Application
Exec=xfce-open --launch MailReader %u
Icon=org.xfce.mailreader
StartupNotify=true
Terminal=false
Categories=Network;X-XFCE;X-Xfce-Toplevel;
Keywords=email;mail;inbox;message;communication;
OnlyShowIn=XFCE;
X-XFCE-MimeType=x-scheme-handler/mailto;message/*;
X-AppStream-Ignore=True
Name=Mail Reader
Comment=Read your email
MimeType=x-scheme-handler/mailto;message/*;
